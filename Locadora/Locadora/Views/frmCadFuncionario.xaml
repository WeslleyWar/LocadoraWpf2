        <TextBox x:Name="txtSalario" HorizontalAlignment="Left" Height="32" Margin="505,226,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="265"/>
        <TextBox x:Name="txtCriadoEm" HorizontalAlignment="Left" Height="32" Margin="505,258,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="265"/>
