        <Button x:Name="btnCancelar" Content="Cancelar" HorizontalAlignment="Left" Margin="505,308,0,0" VerticalAlignment="Top" Width="116" Height="46" Background="#FFFDE8C3" FontWeight="Bold" Click="btnCancelar_Click"/>
        <Label x:Name="lbDataNascimento_Copy" Content="Contratação:" HorizontalAlignment="Left" Margin="10,297,0,0" VerticalAlignment="Top" RenderTransformOrigin="8.944,-2.891" Width="100" Height="31"/>
        <DatePicker x:Name="dtpContratacao" HorizontalAlignment="Left" Margin="95,298,0,0" VerticalAlignment="Top" Width="300" Height="30"/>
        <DatePicker x:Name="dtpNascimento" HorizontalAlignment="Left" Margin="505,157,0,0" VerticalAlignment="Top" Width="265" Height="31"/>

    </Grid>
