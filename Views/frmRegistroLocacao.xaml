<Window x:Class="Locadora.Views.frmRegistroLocacao"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Locadora.Views"
        mc:Ignorable="d"
        Title="Registrar Locação" Height="497.34" Width="848">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FF436368" Offset="1"/>
                <GradientStop Color="#FFB9E8F0" Offset="0.914"/>
            </LinearGradientBrush>
        </Grid.Background>

        <Label x:Name="LbNomeCliente" Content="NOME:" HorizontalAlignment="Left" Margin="10,76,0,0" VerticalAlignment="Top" Width="88" FontSize="15"/>
        <Label x:Name="LbCliente" Content="CLIENTE" HorizontalAlignment="Left" Margin="61,5,0,0" VerticalAlignment="Top" Width="116" FontSize="18" FontWeight="Bold" FontStyle="Italic" FontFamily="Arial Black"/>
        <Label x:Name="LbCpfCliente" Content="CPF:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="88" FontSize="15"/>
        <Label x:Name="LbCnhCliente" Content="CNH:" HorizontalAlignment="Left" Margin="10,111,0,0" VerticalAlignment="Top" Width="88" FontSize="15"/>
        <TextBox x:Name="TxtNomeCliente" HorizontalAlignment="Left" Height="30" Margin="74,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <TextBox x:Name="TxtCpfCliente" HorizontalAlignment="Left" Height="30" Margin="74,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <TextBox x:Name="TxtCnhCliente" HorizontalAlignment="Left" Height="30" Margin="74,111,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <Button x:Name="btnValidarC" Content="VALIDAR" HorizontalAlignment="Left" Margin="10,188,0,0" VerticalAlignment="Top" Width="100" Height="22" Background="#FFEEE1C6" FontWeight="Bold" FontFamily="Arial" FontSize="11" Click="btnValidarC_Click" ClickMode="Press"/>
        <Label x:Name="LbFuncionario" Content="FUNCIONARIO" HorizontalAlignment="Left" Margin="587,7,0,0" VerticalAlignment="Top" Width="158" FontSize="18" FontWeight="Bold" FontStyle="Italic" FontFamily="Arial Black"/>
        <Label x:Name="LbNomeFuncionario" Content="NOME:" HorizontalAlignment="Left" Margin="587,96,0,0" VerticalAlignment="Top" Width="88" FontSize="15"/>
        <Label x:Name="LbCpfFuncionario" Content="CPF:" HorizontalAlignment="Left" Margin="587,58,0,0" VerticalAlignment="Top" Width="88" FontSize="15"/>
        <TextBox x:Name="TxtNomeFuncionario" HorizontalAlignment="Left" Height="30" Margin="680,96,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <TextBox x:Name="TxtCpfFuncionario" HorizontalAlignment="Left" Height="30" Margin="680,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <Button x:Name="btnValidarF" Content="VALIDAR" HorizontalAlignment="Left" Margin="587,188,0,0" VerticalAlignment="Top" Width="100" Height="22" Background="#FFEEE1C6" FontWeight="Bold" FontFamily="Arial" FontSize="11" Click="btnValidarF_Click" ClickMode="Press"/>
        <Label x:Name="LbVeiculo" Content="VEÍCULO" HorizontalAlignment="Left" Margin="342,10,0,0" VerticalAlignment="Top" Width="116" FontSize="18" FontWeight="Bold" FontStyle="Italic" FontFamily="Arial Black"/>
        <Label x:Name="LbStatus" Content="STATUS:" HorizontalAlignment="Left" Margin="280,76,0,0" VerticalAlignment="Top" Width="114" FontSize="15"/>
        <Label x:Name="LbModelo" Content="MODELO:" HorizontalAlignment="Left" Margin="280,111,0,0" VerticalAlignment="Top" Width="114" FontSize="15"/>
        <Label x:Name="LbPlaca" Content="PLACA:" HorizontalAlignment="Left" Margin="282,41,0,0" VerticalAlignment="Top" Width="114" FontSize="15"/>
        <TextBox x:Name="TxtStatus" HorizontalAlignment="Left" Height="30" Margin="370,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <TextBox x:Name="TxtModelo" HorizontalAlignment="Left" Height="30" Margin="370,111,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <TextBox x:Name="TxtPlaca" HorizontalAlignment="Left" Height="30" Margin="370,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" TextChanged="TxtPlaca_TextChanged"/>
        <Button x:Name="btnValidarV" Content="VALIDAR" HorizontalAlignment="Left" Margin="282,188,0,0" VerticalAlignment="Top" Width="100" Height="22" Background="#FFEEE1C6" FontWeight="Bold" FontFamily="Arial" FontSize="11" ClickMode="Press" Click="btnValidarV_Click"/>
        <ComboBox x:Name="CmbTipoVeiculo" Margin="403,145,243.6,351.4">
            <ComboBoxItem Content="Trabalho"/>
            <ComboBoxItem Content="Passeio"/>
            <ComboBoxItem Content="Luxo"/>
        </ComboBox>
        <Label x:Name="LbLocacao" Content="LOCAÇÃO" HorizontalAlignment="Left" Margin="10,232,0,0" VerticalAlignment="Top" Width="158" FontSize="18" FontWeight="Bold" FontStyle="Italic" FontFamily="Arial Black"/>
        <Label x:Name="LbQntdDias" Content="Tempo em Dias:" HorizontalAlignment="Left" Margin="13,263,0,0" VerticalAlignment="Top" Width="130" FontSize="15"/>
        <Label x:Name="LbValorPrevisto" Content="Valor previsto:" HorizontalAlignment="Left" Margin="13,298,0,0" VerticalAlignment="Top" Width="130" FontSize="15"/>
        <Label x:Name="LbCpfFuncionario_Copy" Content="Entrega Prevista:" HorizontalAlignment="Left" Margin="13,368,0,0" VerticalAlignment="Top" Width="130" FontSize="15"/>
        <TextBox x:Name="TxtDiasLocacao" HorizontalAlignment="Left" Height="30" Margin="148,263,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="181"/>
        <TextBox x:Name="TxtValorPrevisto" HorizontalAlignment="Left" Height="30" Margin="148,298,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="181"/>
        <TextBox x:Name="TxtDataPrevistaEntrega" HorizontalAlignment="Left" Height="30" Margin="148,368,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="181"/>
        <Label x:Name="LbCargoFuncionario_Copy1" Content="Inicio Locação:" HorizontalAlignment="Left" Margin="13,333,0,0" VerticalAlignment="Top" Width="130" FontSize="15"/>
        <TextBox x:Name="TxtDataInicio" HorizontalAlignment="Left" Height="30" Margin="148,333,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="181"/>


        <Button x:Name="btnLimpaLocacao" Background="#FFFD9DAE" 
        BorderBrush="Black" BorderThickness="1" 
        Click="btnLimparLocacao" ClickMode="Press" Margin="573,355,111,68" FontFamily="Arial Black" FontSize="18" RenderTransformOrigin="1.349,2.789" Content="Limpar"/>

        <Button x:Name="btnConcluir" Background="#FF67FF17" 
        BorderBrush="Black" BorderThickness="1" 
        ClickMode="Press" Margin="573,257,111,166" FontWeight="Bold" FontFamily="Arial Black" FontSize="18" Click="btnConcluir_Click">
            CONCLUIR
        </Button>
        <Button x:Name="btnLimparCliente" Content="LIMPAR CLIENTE" HorizontalAlignment="Left" Margin="124,188,0,0" VerticalAlignment="Top" Width="100" Height="22" Background="#FFFD9DAE" FontWeight="Bold" FontFamily="Arial" FontSize="11" Click="btnLimpaCliente" ClickMode="Press"/>
        <Button x:Name="btnLimparVeiculo" Content="LIMPAR VEICULO" HorizontalAlignment="Left" Margin="417,188,0,0" VerticalAlignment="Top" Width="100" Height="22" Background="#FFFD9DAE" FontWeight="Bold" FontFamily="Arial" FontSize="11" Click="btnLimpVeiculo" ClickMode="Press"/>
        <Button x:Name="btnLimparFuncionario" Content="LIMPAR FUNCIONARIO" HorizontalAlignment="Left" Margin="730,188,0,0" VerticalAlignment="Top" Width="100" Height="22" Background="#FFFD9DAE" FontWeight="Bold" FontFamily="Arial" FontSize="8" Click="btnLimpaFuncionario" ClickMode="Press"/>
        <Label x:Name="LbDiaria" Content="DIARIA:" HorizontalAlignment="Left" Margin="280,141,0,0" VerticalAlignment="Top" Width="114" FontSize="15"/>
        <TextBox x:Name="TxtDiaria" HorizontalAlignment="Left" Height="30" Margin="370,146,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
    </Grid>
</Window>
